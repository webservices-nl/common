{
  "name": "webservices-nl/common",
  "description": "Webservices Common library for Webservices.nl PHP shared business code",
  "keywords": [
    "common",
    "webservicesnl",
    "webservices.nl"
  ],
  "config": {
    "gitlab-domains": [
      "gitlab.fdmg.org"
    ],
    "preferred-install": "dist"
  },
  "type": "library",
  "require": {
    "php": "^5.4 || ^7",
    "doctrine/collections": "^1.3",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "mockery/mockery": "^0.9.4",
    "league/factory-muffin": "^2.1",
    "league/factory-muffin-faker": "~1.0-beta",
    "sensiolabs/security-checker": "^4",
    "friendsofphp/php-cs-fixer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "WebservicesNl\\Common\\": "src/Common/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebservicesNl\\Test\\": "test/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "WebservicesNl",
      "email": "tech@webservices.nl",
      "homepage": "https://webview.webservices.nl"
    }
  ]
}
